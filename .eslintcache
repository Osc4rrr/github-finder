 = async(username) => {\n        setLoading(); \n        const res = await axios.get(`https://api.github.com/users/${username}?client_id=${process.env.REACT_APP_GITHUB_CLIENT_ID}&client_secret=${process.env.REACT_APP_GITHUB_CLIENT_SECRET}`); \n        \n        dispatch({\n            type: GET_USER, \n            payload: res.data\n        }); \n    };\n\n    //Get Repos \n\n\n    //Clear Users\n    const clearUsers = () => dispatch({type: CLEAR_USERS}); \n\n    //Set Loading\n    const setLoading = () => dispatch({\n        type: SET_LOADING\n    }); \n\n    return <GithubContext.Provider\n        value={{\n            users: state.users, \n            user: state.user, \n            repos: state.repos, \n            loading: state.loading, \n            searchUsers, \n            clearUsers, \n            getUser\n        }}\n    >\n        {props.children}\n    </GithubContext.Provider>\n}\n\nexport default GithubState; ","/Users/oscardiaz/Desktop/Traversy media/React-course/github-finder/src/context/github/githubContext.js",[],"/Users/oscardiaz/Desktop/Traversy media/React-course/github-finder/src/context/github/githubReducer.js",["76","77","78","79"],"/Users/oscardiaz/Desktop/Traversy media/React-course/github-finder/src/context/types.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":13,"column":10,"nodeType":"86","messageId":"87","endLine":13,"endColumn":22},{"ruleId":"84","severity":1,"message":"88","line":17,"column":10,"nodeType":"86","messageId":"87","endLine":17,"endColumn":17},{"ruleId":"84","severity":1,"message":"89","line":11,"column":5,"nodeType":"86","messageId":"87","endLine":11,"endColumn":14},{"ruleId":"84","severity":1,"message":"90","line":13,"column":5,"nodeType":"86","messageId":"87","endLine":13,"endColumn":14},{"ruleId":"84","severity":1,"message":"91","line":14,"column":5,"nodeType":"86","messageId":"87","endLine":14,"endColumn":17},{"ruleId":"84","severity":1,"mes